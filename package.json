{
  "name": "ngx-dynamic-components",
  "version": "0.0.1",
  "description": "NGX Dynamic Components is a dynamic configuration driven web page builder. It is designed to build anything from a single one form web page to more complex web app.",
  "author": "FalconSoft Ltd",
  "repository": {
    "type": "git",
    "url": "https://github.com/FalconSoft/ngx-dynamic-components"
  },
  "homepage": "https://falconsoft.github.io/ngx-dynamic-components/",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "setup": "npm i && npm run build:libs",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:libs": "ng build core && ng build material && ng build bootstrap && ng build tools",
    "build:gh": "ng build --prod --base-href='/ngx-dynamic-components/'",
    "deploy:gh": "npm run build:libs && npm run build:gh && npx ngh --dir=dist/ngx-dynamic-components/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.1.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@ng-select/ng-select": "^3.0.3",
    "angular-split": "^3.0.2",
    "bootstrap": "^4.3.1",
    "hammerjs": "^2.0.8",
    "json-formatter-js": "^2.2.1",
    "ng2-dragula": "^2.1.1",
    "ngx-bootstrap": "^5.1.0",
    "ngx-monaco-editor": "^7.0.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
