{
  "name": "ngx-dynamic-components",
  "version": "0.2.13",
  "description": "NGX Dynamic Components is a configuration based dynamic components library for Angular. That allows you to rapidly create dynamic forms or any other mobile-friendly web layouts.",
  "author": "FalconSoft Ltd",
  "repository": {
    "type": "git",
    "url": "https://github.com/FalconSoft/ngx-dynamic-components"
  },
  "homepage": "https://falconsoft.github.io/ngx-dynamic-components/",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "setup": "npm i && npm run build:libs",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:core": "ng build core --prod && ng build bootstrap --prod",
    "build:libs": "ng build core --prod && ng build material --prod && ng build bootstrap --prod && ng build tools --prod",
    "public:publish": "cd dist/@ngx-dynamic-components/core && npm publish && cd ../bootstrap && npm publish && cd ../tools && npm publish && cd ../../../",
    "build:publish": "npm run build:libs && npm run public:publish",
    "build:gh": "ng build --prod --base-href=/ngx-dynamic-components/",
    "deploy:gh": "npm run build:libs && npm run build:gh && npx ngh --dir=dist/ngx-dynamic-components/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "@ng-select/ng-select": "^6.1.0",
    "ace-builds": "^1.4.12",
    "angular-resizable-element": "^3.3.4",
    "angular-split": "^5.0.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.8.1",
    "font-awesome": "^4.7.0",
    "isomorphic-xml2js": "0.1.3",
    "json-formatter-js": "^2.3.3",
    "jspython-interpreter": "^2.0.6",
    "ngx-bootstrap": "^6.2.0",
    "ngx-markdown": "^11.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/builder": "^2.0.0",
    "@angular-eslint/eslint-plugin": "^2.0.0",
    "@angular-eslint/eslint-plugin-template": "^2.0.0",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "^2.0.0",
    "@angular/cli": "^11.2.5",
    "@angular/compiler-cli": "^11.2.6",
    "@angular/language-service": "^11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.11",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.2",
    "typescript": "~4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
