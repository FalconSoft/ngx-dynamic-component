{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "preserveWhitespaces": true
  },
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@ngx-dynamic-components/core": [
        "dist/@ngx-dynamic-components/core",
        // "projects/core/src/public_api.ts"
      ],
      "@ngx-dynamic-components/core/*": [
        "dist/@ngx-dynamic-components/core/*"
      ],
      "@ngx-dynamic-components/material": [
        "dist/@ngx-dynamic-components/material",
        // "projects/material/src/public_api.ts"
      ],
      "@ngx-dynamic-components/material/*": [
        "dist/@ngx-dynamic-components/material/*"
      ],
      "@ngx-dynamic-components/bootstrap": [
        "dist/@ngx-dynamic-components/bootstrap",
        // "projects/bootstrap/src/public-api.ts"
      ],
      "@ngx-dynamic-components/bootstrap/*": [
        "dist/@ngx-dynamic-components/bootstrap/*"
      ],
      "@ngx-dynamic-components/tools": [
        // "dist/@ngx-dynamic-components/tools",
        "projects/tools/src/public_api.ts"
      ],
      "@ngx-dynamic-components/tools/*": [
        "dist/@ngx-dynamic-components/tools/*"
      ],
      "core": [
        "dist/@ngx-dynamic-components/core"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "material": [
        "dist/@ngx-dynamic-components/material"
      ],
      "material/*": [
        "dist/material/*"
      ],
      "tools": [
        "dist/@ngx-dynamic-components/tools"
      ],
      "tools/*": [
        "dist/tools/*"
      ],
      "bootstrap": [
        "dist/@ngx-dynamic-components/bootstrap"
      ],
      "bootstrap/*": [
        "dist/bootstrap/*"
      ],
      "core-js/es7/reflect": [
        "node_modules/core-js/proposals/reflect-metadata"
      ]
    }
  }
}
